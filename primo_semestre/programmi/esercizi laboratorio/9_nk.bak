;problema 9
(define abc
  (list "A" "B" "C" "D" "E" "F" "G" "H" "I" "L" "M" "N" "O" "P" "Q" "R" "S" "T" "V" "X")
  )
(define cesare
  (lambda (n k)
    (if (= (string-length k) 0) ""
        (let ((x (+(check-abc (substring k 0 1) abc)n)))

        (string-append(list-ref abc
                       (if (< 19 x ) (- x 20)
                        x))
                       (cesare n (substring k 1)) )
          

        )
          ))
  )

(define check-abc
  (lambda(k alf)
    (if (string=? k (car alf))0
        (+ 1 (check-abc k (cdr alf)))
       )))

